function ret = findLongestSubstring(s)
    len = 0;
    l = 1;
    pivot = 1;
    while len <= length(s) - l
        table = zeroes(255, 1);
        pivot = l;
        while pivot <= length(s) - 1
            if table(pivot) == 1
                break
            else
                table(pivot) = 1;
                pivot = pivot + 1;
            end
        
    end
end